            "<script src=\"https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.4/js/select2.min.js\"></script>",
            "<script>    \n\tvar surveyJSON = { \n        title: \"**Solicita tu Cuenta** <br/> Llena el siguiente formulario\",\n        cookieName: \"confiame-registro-confialiado\",\n        showQuestionNumbers: \"off\",\n        requiredText: \"*\",\n        pages: [\n            {\n                title: \"Datos Personales\",\n                description: \"En confíame tus datos son lo más importante, es por esto que los tratamos con cuidado y **no los vendemos a terceros**.\",\n                name:\"datos-personales\",\n                questions: [\n                    {\n                        \"name\": \"name\",\n                        \"type\": \"text\",\n                        \"title\": \"Nombre:\",\n                        \"placeHolder\": \"Juan Pablo\",\n                        \"isRequired\": true\n                    },\n                    {\n                        \"name\": \"last-name\",\n                        \"type\": \"text\",\n                        \"title\": \"Apellido:\",\n                        \"placeHolder\": \"Pérez\",\n                        \"isRequired\": true\n                    },\n                    {\n                        \"name\": \"birthdate\",\n                        \"type\": \"text\",\n                        \"inputType\": \"date\",\n                        \"title\": \"Fecha de nacimiento:\",\n                        \"isRequired\": true,\n                        validators: [\n                            {\n                                type: \"expression\",\n                                text: \"Debes ser mayor de edad\",\n                                expression: \"{age} > 17\"\n                            },\n                            {\n                                type: \"expression\",\n                                text: \"Debes ser un ser humano\",\n                                expression: \"{age} < 90\"\n                            }\n                        ],\n                    },\n                    {\n                        \"name\": \"email\",\n                        \"type\": \"text\",\n                        \"inputType\": \"email\",\n                        \"title\": \"E-mail:\",\n                        \"placeHolder\": \"jon.snow@nightwatch.org\",\n                        \"isRequired\": true,\n                        \"validators\": [\n                            {\n                                \"type\": \"email\"\n                            }\n                        ]\n                    },\n                    {\n                        \"name\": \"phone\",\n                        \"type\": \"text\",\n                        \"inputMask\": \"phone\",\n                        \"title\": \"Teléfono:\",\n                        \"isRequired\": true,\n                        \"inputFormat\": \"🇲🇽 +52 (###)-###-####\",\n                    }\n                ]\n            },\n            {\n                name: 'location',\n                title: \"¿Donde vives?\",\n                description: 'Por favor compartenos la zona en la que vives',\n                questions: [\n                    {\n                        type: \"text\",\n                        name: \"zip\",\n                        title: \"Código Postal\"\n                    },\n                    {\n                        type: \"radiogroup\",\n                        name: \"postCodeList\",\n                        maxTextLengh: 5,\n                        //startWithNewLine: false,\n                        title: \"Colonia\",\n                        visibleIf: \"{zip} notempty\",\n                        choicesByUrl: {\n                            url: \"https://api-sepomex.hckdrk.mx/query/info_cp/{zip}\",\n                            valueName: \"response.asentamiento\"\n                        }\n                    },\n                    \n                ]\n            },\n            {\n                name: 'skills',\n                title: 'Tus habilidades',\n            \tdescription: 'Por favor selecciona todas las habilidades que posees',\n                questions: [\n                    {\n                    \ttype: 'dropdown',\n                        name: 'education-level',\n                        title: 'Nivel de escolaridad',\n                        isRequired: true,\n                        choices: [\n                            {\n                                value: 1,\n                                text: 'Primaria'\n                            },\n                            {\n                                value: 2,\n                                text: 'Secundaria'\n                            },\n                            {\n                                value: 3,\n                                text: 'Preparatoria / Bachillerato'\n                            },\n                            {\n                                value: 4,\n                                text: 'Licenciatura / Ingeniería'\n                            },\n                            {\n                                value: 5,\n                                text: 'Maestría'\n                            },\n                            {\n                                value: 6,\n                                text: 'Doctorado'\n                            }\n                        ]\n                    },\n                    {\n                    \ttype: 'dropdown',\n                        name: 'universityName',\n                        visibleIf: '{education-level} > 3',\n                        title: 'Nombre de tu universidad',\n                        description: 'Si tienes maestría o doctorado, separa con una coma todas tus universidades',\n                        choicesOrder: 'asc',\n                        choicesByUrl: {\n                            url: 'https://sheet.best/api/sheets/1750637e-6da2-40cc-b206-bcc67022ebcb',\n                            valueName: 'name'\n                        }\n                    },\n                    {\n                    \ttype: 'text',\n                        name: 'degree',\n                        visibleIf: '{education-level} > 3',\n                        title: 'Título profesional',\n                        description: 'Si tienes más de uno, sepáralo por una coma'\n                    },\n                    {\n                    \ttype: 'dropdown',\n                        name: 'experience',\n                        visibleIf: '{education-level} > 3',\n                        title: 'Años de Experiencia Profesional',\n                        choices: [\n                            {\n                                value: 1,\n                                text: '0-1 año - Recien graduado'\n                            },\n                            {\n                                value: 2,\n                                text: '1-3 años - Novato'\n                            },\n                            {\n                                value: 3,\n                                text: '3-5 años - Intermedio'\n                            },\n                            {\n                                value: 4,\n                                text: '5-10 años - Avanzado'\n                            },\n                            {\n                                value: 5,\n                                text: '5-10+ años - Experto'\n                            }\n                        ]\n                    },\n                    {\n                        type: 'checkbox',\n                        name: 'jobType',\n                        title: 'Tipos de trabajo que te gustaría realizar',\n                        colCount: 3,\n                        choices: [\n                            'Trabajos en la ciudad',\n                            'Servicios a domicilio',\n                            'Trabajo a distancia'\n                        ]\n            